/* authList.wxss */
@import "../../bind/bind.wxss";

.card {
  width: 700rpx;
  margin-top: 20rpx;
  color: black;
  margin-left: 25rpx;
  margin-right: 25rpx;
  background-color: white;
  box-shadow: 0px 5rpx 16rpx #999;
  float: left;
}

.card-img-view {
  float: left;
  width: 180rpx;
  height: 100%;
  text-align: center;
}

.card-img {
  width: 120rpx;
  height: 120rpx;
  margin: 24rpx auto;
}

.card-body {
  float: left;
  width: 520rpx;
  box-sizing: border-box;
}

.card-body-title {
  margin-top: 20rpx;
}

.card-body-desc {
  margin-bottom: 20rpx;
}

.card-body text {
  display: block;
  width: 520rpx;
}

.card-body-title {
  font-weight: bold;
}

.defaultAuth {
  background-image: url('data:image/svg+xml;base64,');
  background-size: 64rpx 64rpx;
  background-position: right;
  background-repeat: no-repeat;
  position: relative;
  display: block;
  float: right;
  height: 64rpx;
  width: 64rpx;
  z-index: 10;
}

.scroll-fixed-has-button {
  position: fixed;
  bottom: 192rpx;
  top: 0rpx;
  left: 0rpx;
  right: 0rpx;
}

.scroll-fixed-no-button {
  position: fixed;
  top: 0rpx;
  left: 0rpx;
  right: 0rpx;
  bottom: 86rpx;
}
